{
  "name": "@luxdefi/luxtown",
  "version": "1.0.0",
  "author": "Lux Partners Limited <hi@lux.partners> (https://lux.partners)",
  "license": "MIT",
  "keywords": [
    "ethereum",
    "react",
    "uniswap",
    "workspaces",
    "yarn"
  ],
  "private": true,
  "scripts": {
    "build": "yarn build:app",
    "build:app": "yarn workspace @luxtown/app build",
    "build:lab": "yarn workspace @luxtown/lab build",
    "clean": "rm -rf packages/app/build && rm -rf packages/contracts/artifacts && rm -rf packages/contracts/cache && rm -rf packages/contracts/types",
    "dev": "yarn workspace @luxtown/app dev",
    "console": "yarn workspace @luxtown/contracts console",
    "upload": "yarn workspace @luxtown/moralis upload",
    "uploaded": "yarn workspace @luxtown/moralis uploaded",
    "start": "yarn workspace @luxtown/app start",
    "start:lab": "yarn workspace @luxtown/lab start",
    "account": "yarn workspace @luxtown/contracts account",
    "accounts": "yarn workspace @luxtown/contracts accounts",
    "balance": "yarn workspace @luxtown/contracts balance",
    "chain": "yarn workspace @luxtown/contracts chain",
    "chain2": "yarn workspace @luxtown/contracts chain2",
    "compile": "yarn workspace @luxtown/contracts compile",
    "deploy": "yarn workspace @luxtown/contracts deploy",
    "deploy:game": "yarn workspace @luxtown/contracts deploy:game",
    "deploy:mainnet": "yarn workspace @luxtown/contracts deploy:mainnet",
    "deploy:testnet": "yarn workspace @luxtown/contracts deploy:testnet",
    "configure": "yarn workspace @luxtown/contracts configure",
    "configure:testnet": "yarn workspace @luxtown/contracts configure:testnet",
    "configure:mainnet": "yarn workspace @luxtown/contracts configure:mainnet",
    "export": "yarn workspace @luxtown/contracts export",
    "flatten": "yarn workspace @luxtown/contracts flatten",
    "fork": "yarn workspace @luxtown/contracts fork",
    "fundedwallet": "yarn workspace @luxtown/contracts fundedwallet",
    "generate": "yarn workspace @luxtown/contracts generate",
    "matrix": "yarn workspace @luxtown/contracts matrix",
    "mineContractAddress": "yarn workspace @luxtown/contracts mineContractAddress",
    "node": "yarn workspace @luxtown/contracts chain",
    "send": "yarn workspace @luxtown/contracts send",
    "test": "yarn workspace @luxtown/contracts test",
    "wallet": "yarn workspace @luxtown/contracts wallet",
    "drop": "yarn workspace @luxtown/moralis drop",
    "functions": "yarn workspace @luxtown/moralis functions",
    "logs": "yarn workspace @luxtown/moralis logs",
    "plugins": "yarn workspace @luxtown/moralis plugins",
    "proxy": "yarn workspace @luxtown/moralis proxy",
    "watch": "yarn workspace @luxtown/moralis watch",
    "lab": "yarn workspace @luxtown/lab start",
    "subgraph:codegen": "yarn workspace @luxtown/subgraph codegen",
    "subgraph:prepare-local": "yarn workspace @luxtown/subgraph prepare:local",
    "subgraph:prepare-testnet": "yarn workspace @luxtown/subgraph prepare:testnet",
    "subgraph:prepare-mainnet": "yarn workspace @luxtown/subgraph prepare:mainnet",
    "subgraph:build": "yarn workspace @luxtown/subgraph build",
    "subgraph:create-local": "yarn workspace @luxtown/subgraph create:local",
    "subgraph:deploy-local": "yarn workspace @luxtown/subgraph deploy:local",
    "subgraph:deploy-testnet": "yarn workspace @luxtown/subgraph deploy:testnet",
    "subgraph:deploy-mainnet": "yarn workspace @luxtown/subgraph deploy:mainnet",
    "subgraph:start": "yarn workspace @luxtown/subgraph start",
    "graphnode:start": "yarn workspace @luxtown/graphnode start",
    "graphnode:docker": "yarn workspace @luxtown/graphnode docker",
    "graphnode:clean": "yarn workspace @luxtown/graphnode clean",
    "format": "yarn workspace @luxtown/app format && yarn workspace @luxtown/contracts format && yarn workspace @luxtown/moralis format",
    "verify:testnet": "yarn workspace @luxtown/contracts verify:testnet"
  },
  "workspaces": {
    "packages": [
      "app",
      "contracts",
      "graphnode",
      "lab",
      "moralis",
      "packages"
    ],
    "nohoist": [
      "**/@openzeppelin/contracts-upgradeable/**",
      "**/@openzeppelin/contracts",
      "**/@luxdefi/hardhat",
      "**/@luxdefi/solidity"
    ]
  },
  "resolutions": {
    "node-hid": "./resolutions/node-hid",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "dependencies": {}
}
